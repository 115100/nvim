{
  "coc.preferences.formatOnSaveFiletypes": [
    "json",
    "python",
    "rust",
    "yaml"
  ],
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "filetypes": ["go"]
    },
    "terraform": {
      "command": "terraform-lsp",
      "filetypes": ["terraform"],
      "initializationOptions": {}
    }
  },
  "python.formatting.provider": "black",
  "python.jediEnabled": false,
  "python.pythonPath": "python3",
  "yaml.format.enable": true
}
